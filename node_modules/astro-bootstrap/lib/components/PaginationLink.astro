---
import ActiveLink from '../utils/ActiveLink.astro';
// className = '' as not using class:list
const { href, class: className = '' } = Astro.props;
export interface Props {
  href?: string;
  disabled?: boolean;
  class?: string;
}
---

<ActiveLink
  {href}
  class={`page-link ${className}`}
>
  <slot />
</ActiveLink>
<!-- class:list={['page-link', className]} -->
