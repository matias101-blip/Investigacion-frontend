---
interface Props {
	title: string	
}
const {title} = Astro.props

import 'bootstrap/dist/css/bootstrap.min.css'
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
</head>
<body>
    <div class="navbar navbar-expand-lg bg-dark" data-bs-theme="dark">
        <a class="navbar-brand">ConcepCode</a>
        <ul class="navbar-nav">
            <li class="nav-item">
                <a href="/" class="nav-link"><i class="fa-solid fa-house"></i>  Inicio</a>
            </li>
        </ul>
    </div>
    
    <slot/>

    <script>
        import 'bootstrap/dist/js/bootstrap.bundle.min.js'
    </script>
    <script src="https://kit.fontawesome.com/fbdfb4841c.js" crossorigin="anonymous"></script>
</body>
</html>